<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
	<TargetFrameworks Condition="'$(LibraryFrameworks)'==''">net45;net40;netcoreapp3.1;net5.0-windows</TargetFrameworks>
    <TargetFrameworks Condition="'$(LibraryFrameworks)'!=''">$(LibraryFrameworks)</TargetFrameworks>
    <UseWPF>true</UseWPF>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	<PackageId>DSaladin.FontAwesome.WPF</PackageId>
    <Authors>DSaladin</Authors>
    <Product>FontAwesome WPF</Product>
    <Description>Use all free FontAwesome Icons in WPF with custom controls and integrated properties</Description>
    <Copyright>Copyright Â© DSaladin 2021</Copyright>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageIcon>FontAwesomeLogo.png</PackageIcon>
    <RepositoryUrl>https://github.com/DSaladinCH/fontawesome-wpf</RepositoryUrl>
    <PackageTags>FontAwesome,Icon</PackageTags>
    <SignAssembly>False</SignAssembly>
    <AssemblyOriginatorKeyFile>FontAwesomeWPF.pfx</AssemblyOriginatorKeyFile>
    <ApplicationIcon></ApplicationIcon>
    <PackageProjectUrl>https://github.com/DSaladinCH/fontawesome-wpf</PackageProjectUrl>
    <PackageReleaseNotes>Added support for ContentControl rotation</PackageReleaseNotes>
    <RootNamespace>DSaladin.FontAwesome.WPF</RootNamespace>
    <AssemblyVersion>5.15.4.3</AssemblyVersion>
    <FileVersion>5.15.4.3</FileVersion>
    <Version>5.15.4.3</Version>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <AssemblyName>DSaladin.FontAwesome.WPF</AssemblyName>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <TargetFrameworks>net45;net40;netcoreapp3.1;net5.0-windows;net6.0-windows</TargetFrameworks>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(TargetFramework)|$(Platform)'=='Release|net45|AnyCPU'">
    <DebugType>full</DebugType>
    <DebugSymbols>true</DebugSymbols>
    <Optimize>true</Optimize>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="fonts\**" />
    <EmbeddedResource Remove="fonts\**" />
    <None Remove="fonts\**" />
    <None Include="..\..\LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <Page Remove="fonts\**" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\README.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="FontAwesomeLogo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
